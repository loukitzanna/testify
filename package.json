{
  "name": "testify",
  "version": "1.0.0",
  "description": "A repo with different test frameworks",
  "main": "index.js",
  "author": "Anna Loukianova",
  "license": "MIT",
  "scripts": {
    "start": "parcel serve --open src/index.html",
    "build": "parcel build --public-url ./ src/index.html",
    "test": "mocha --require babel-core/register --require ignore-styles --require setup.js src/**/*.test.js src/**/**/*.test.js --recursive"
  },
  "dependencies": {
    "react": "^16.2.0",
    "react-dom": "^16.2.0"
  },
  "devDependencies": {
    "@babel/core": "7.0.0-beta.42",
    "@babel/preset-env": "^7.0.0-beta.42",
    "babel-core": "^6.26.0",
    "babel-eslint": "^8.2.2",
    "babel-plugin-syntax-object-rest-spread": "^6.13.0",
    "babel-plugin-transform-class-properties": "^6.24.1",
    "babel-plugin-transform-object-rest-spread": "^6.26.0",
    "babel-preset-env": "^1.6.1",
    "babel-preset-react": "^6.24.1",
    "chai": "^4.1.2",
    "enzyme": "^3.3.0",
    "enzyme-adapter-react-16": "^1.1.1",
    "eslint": "^4.19.1",
    "eslint-config-standard": "^11.0.0",
    "eslint-plugin-import": "^2.9.0",
    "eslint-plugin-node": "^6.0.1",
    "eslint-plugin-promise": "^3.7.0",
    "eslint-plugin-react": "^7.7.0",
    "eslint-plugin-standard": "^3.0.1",
    "ignore-styles": "^5.0.1",
    "jsdom": "^11.6.2",
    "mocha": "^5.0.5",
    "node-sass": "^4.8.3",
    "react-test-renderer": "^16.2.0"
  }
}
