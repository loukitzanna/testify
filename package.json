{
  "name": "testify",
  "version": "1.0.0",
  "description": "A repo with different test frameworks",
  "main": "index.js",
  "author": "Anna Loukianova",
  "license": "MIT",
  "engines": {
    "node": ">=8.9.4"
  },
  "scripts": {
